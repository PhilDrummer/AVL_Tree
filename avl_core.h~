#include <stdio.h>
#include <stdlib.h>

typedef struct tree_node_s {
  int key;
  void *data;
  struct tree_node_s *left_child;
  struct tree_node_s *right_child;
} Node;

typedef struct avl_tree_s {
  int height;
  struct tree_node_s *root;
} AvlTree;

int key_insert(int key);

int key_delete(int key);

int key_lookup(int key);

int val_insert(int val);

int val_lookup(int val);

int val_delete(int val);

int get_height();
